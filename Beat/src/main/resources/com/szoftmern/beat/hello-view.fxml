<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="217.0" prefWidth="241.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.szoftmern.beat.MusicPlayer">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <Button mnemonicParsing="false" onAction="#volUp" text="volUp" />
   <Label fx:id="Volumelabel" text="Volume: 100% " />
   <Button mnemonicParsing="false" onAction="#volDown" text="volDown" />
   <Button mnemonicParsing="false" onAction="#mute" text="Mute" />
   <Button mnemonicParsing="false" onAction="#pause" text="Pause/Continue" />
   <Button mnemonicParsing="false" onAction="#stop" text="Stop" />
   <Label fx:id="statuslabel" text=" " />
  <Button fx:id="playbutton" onAction="#testMusicPlayer" prefHeight="26.0" prefWidth="93.0" text="Play Music" />
</VBox>
